{
  "name": "loma-blog",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon app.js",
    "protect": "node ./utils/auto.js",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint public/src/**/*.js -c .eslintrc --fix",
    "lint:style": "stylelint \"public/src/**/*.less\" --syntax less --fix",
    "format": "prettier --write public/src/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "public/src/**/*.less": [
      "npm run format",
      "npm run lint:style",
      "git add"
    ],
    "public/src/**/*.{js,jsx}": [
      "npm run format",
      "npm run lint:es",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loma18/miniprogram.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.1",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "getmac": "^1.4.6",
    "http": "0.0.0",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "webpack-dev-server": "^3.8.0"
  },
  "bugs": {
    "url": "https://github.com/loma18/miniprogram/issues"
  },
  "homepage": "https://github.com/loma18/miniprogram#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
